{
    "name": "row-level-security-extension",
    "version": "0.0.0",
    "main": "dist/extension.js",
    "description": "Row Level Security Extension",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "build": "npm run generate && npm run compile",
        "compile": "tsc --build && esbuild dist/extension.js --bundle --outfile=dist/row-level-security-extension.js --target=es6",
        "generate": "bash generate-description.sh",
        "lint": "eslint ./src/",
        "clean": "rm -rf dist/",
        "test": "jest --silent",
        "test-watch": "jest --watch"
    },
    "dependencies": {
        "@exasol/extension-manager-interface": "0.5.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "esbuild": "^0.25.1",
        "eslint": "9.22.0",
        "jest": "29.7.0",
        "ts-jest": "^29.2.6",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.26.0"
    }
}
